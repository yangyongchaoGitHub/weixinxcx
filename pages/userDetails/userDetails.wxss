page {
    background-color: #F7F7F7;
}

.root {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction:column;
    align-items: center;
    margin-top: 80rpx;
}

.bottomLine {
        width: 98%;
        height: 1rpx;
        background-color: #F7F7F7;
}

.head_parent {
    margin-top: 20rpx;
    width: 100%;
    height: 240rpx;
    display: flex;
    flex-direction: row;
    background-color: #FFF0F5;
}

.head {
    height: 130rpx;
    width: 130rpx;
    margin-right: 30rpx;
}

.head_left {
    height: 100%;
    width: 50%;
    display: flex;
    align-items: center;
    padding-left: 30rpx;
}

.head_right {
    height: 100%;
    width: 50%;
    display: flex;
    align-items: center;
    justify-content: flex-end;
}

.name_parent {
    width: 100%;
    height: 100rpx;
    display: flex;
    flex-direction: row;
    background-color: #FFF0F5;
}

.name_left {
    height: 100%;
    width: 50%;
    display: flex;
    align-items: center;
    padding-left: 30rpx;
}

.name_right {
    height: 100%;
    width: 50%;
    display: flex;
    justify-content: flex-end;
    align-items: center;
    margin-right: 30rpx;
}

.phone_parent {
    width: 100%;
    height: 100rpx;
    display: flex;
    flex-direction: row;
    background-color: #FFF0F5;
}

.phone_left {
    height: 100%;
    width: 50%;
    display: flex;
    align-items: center;
    padding-left: 30rpx;
}

.phone_right {
    height: 100%;
    width: 50%;
    display: flex;
    justify-content: flex-end;
    align-items: center;
    margin-right: 30rpx;
}

.changePswd {
    width: 100%;
    height: 100rpx;
    display: flex;
    flex-direction: row;
    background-color: #FFF0F5;
    margin-top: 50rpx;
}

.changePswd_text {
    display: flex;
    align-items: center;
    padding-left: 30rpx;
}

.logout {
    margin-top: 50rpx;
    width: 100%;
    height: 100rpx;
    background-color: #FF8C69;
}

.logout_text {
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
}